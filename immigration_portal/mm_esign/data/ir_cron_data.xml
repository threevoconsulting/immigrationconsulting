<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Cron: Expire Old Signature Requests -->
        <record id="ir_cron_expire_esign_requests" model="ir.cron">
            <field name="name">E-Sign: Expire Old Requests</field>
            <field name="model_id" ref="model_mm_esign_request"/>
            <field name="state">code</field>
            <field name="code">model._cron_expire_requests()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active" eval="True"/>
        </record>
        
    </data>
</odoo>
