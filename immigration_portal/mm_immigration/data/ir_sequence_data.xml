<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Case Reference Sequence -->
        <record id="seq_immigration_case" model="ir.sequence">
            <field name="name">Immigration Case Sequence</field>
            <field name="code">mm.immigration.case</field>
            <field name="prefix">TMM-%(year)s-</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
